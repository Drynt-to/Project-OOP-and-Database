<?import javafx.scene.text.Font?>

<AnchorPane fx:id="frmPane" prefHeight="580.0" prefWidth="1024.0" styleClass="window" stylesheets="@main_view.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projekpbobd.controllers.PegawaiController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <HBox prefHeight="34.0" prefWidth="1024.0" styleClass="welcome-bar" stylesheets="@dataMahasiswa_view.css" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="back" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="71.0" styleClass="button-back" stylesheets="@dataMahasiswa_view.css">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@images/tombolBack.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                            <HBox.margin>
                                <Insets left="10.0" top="40.0" />
                            </HBox.margin>
                        </Button>
                        <Label styleClass="label-welcome" stylesheets="@dataMahasiswa_view.css" text="Data Pegawai">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="40.0" left="10.0" top="40.0" />
                            </HBox.margin>
                        </Label>
                    </children>
                </HBox>
            </top>
            <center>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="content-data-mahasiswa" stylesheets="@dataMahasiswa_view.css" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </BorderPane.margin>
                    <children>
                        <TableView fx:id="tblPegawai" layoutX="20.0" layoutY="44.0" prefHeight="470.0" prefWidth="521.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="410.0" AnchorPane.topAnchor="45.0">
                            <columns>
                                <TableColumn prefWidth="55.0" text="ID Pegawai" />
                                <TableColumn prefWidth="141.0" text="ID Staff Koperasi" />
                                <TableColumn prefWidth="116.0" text="Nama Depan" />
                                <TableColumn prefWidth="123.0" text="Nama Belakang" />
                                <TableColumn prefWidth="118.0" text="No Telpon" />
                            </columns>
                        </TableView>
                        <Button fx:id="delete" layoutX="835.0" layoutY="379.0" mnemonicParsing="false" onAction="#delete" prefHeight="20.0" prefWidth="60.0" styleClass="button-content-data" stylesheets="@dataMahasiswa_view.css" text="Delete" AnchorPane.leftAnchor="860.0" AnchorPane.topAnchor="300.0" />
                        <Button fx:id="update" layoutX="771.0" layoutY="380.0" mnemonicParsing="false" onAction="#update" prefHeight="20.0" prefWidth="40.0" styleClass="button-content-data" stylesheets="@dataMahasiswa_view.css" text="Edit" AnchorPane.leftAnchor="810.0" AnchorPane.topAnchor="300.0" />
                        <Button fx:id="save" layoutX="697.0" layoutY="380.0" mnemonicParsing="false" onAction="#save" prefHeight="20.0" prefWidth="50.0" styleClass="button-content-data" stylesheets="@dataMahasiswa_view.css" text="Save" AnchorPane.leftAnchor="750.0" AnchorPane.topAnchor="300.0" />
                        <Label layoutX="680.0" layoutY="85.0" styleClass="text-field-label" stylesheets="@dataMahasiswa_view.css" text="Nama Depan" AnchorPane.leftAnchor="620.0" AnchorPane.topAnchor="100.0" />
                        <Label layoutX="675.0" layoutY="158.0" styleClass="text-field-label" stylesheets="@dataMahasiswa_view.css" text="Nama Belakang" AnchorPane.leftAnchor="800.0" AnchorPane.topAnchor="100.0" />
                        <Label layoutX="680.0" layoutY="228.0" styleClass="text-field-label" stylesheets="@dataMahasiswa_view.css" text="Nomor Telpon" AnchorPane.leftAnchor="620.0" AnchorPane.topAnchor="160.0" />
                        <TextField fx:id="txtNamaDepan" layoutX="676.0" layoutY="105.0" prefHeight="20.0" prefWidth="150.0" AnchorPane.leftAnchor="620.0" AnchorPane.topAnchor="120.0" />
                        <TextField fx:id="txtNamaBelakang" layoutX="675.0" layoutY="176.0" prefHeight="20.0" prefWidth="150.0" AnchorPane.leftAnchor="800.0" AnchorPane.topAnchor="120.0" />
                        <TextField fx:id="txtNoTelp" layoutX="675.0" layoutY="246.0" prefHeight="20.0" prefWidth="150.0" AnchorPane.leftAnchor="620.0" AnchorPane.topAnchor="180.0" />
                        <ImageView fitHeight="32.0" fitWidth="35.0" layoutX="933.0" layoutY="377.0" onMouseClicked="#export" preserveRatio="true" AnchorPane.leftAnchor="940.0" AnchorPane.topAnchor="293.0">
                            <image>
                                <Image url="@images/Export.png" />
                            </image>
                        </ImageView>
                        <TextField fx:id="idStaff" layoutX="680.0" layoutY="313.0" prefHeight="20.0" prefWidth="150.0" AnchorPane.leftAnchor="800.0" AnchorPane.topAnchor="180.0" />
                        <Label layoutX="680.0" layoutY="295.0" styleClass="text-field-label" stylesheets="@dataMahasiswa_view.css" text="Id Staff Koperasi" AnchorPane.leftAnchor="800.0" AnchorPane.topAnchor="160.0" />
                        <Label layoutX="20.0" layoutY="14.0" styleClass="search-title-text-field" stylesheets="@dataMahasiswa_view.css" text="Search" AnchorPane.leftAnchor="20.0" />
                        <TextField fx:id="searchBar" layoutX="66.0" layoutY="10.0" onAction="#search" prefHeight="8.0" prefWidth="200.0" styleClass="search-text-field" stylesheets="@dataMahasiswa_view.css" AnchorPane.leftAnchor="80.0" />
                    </children>
                </AnchorPane>
            </center>
        </BorderPane>
        <Label styleClass="font-joinStaff-dulu" stylesheets="@dataMahasiswa_view.css" text="Ayo ! Join Staff Koperasi Terlebih Dahulu" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="35.0" />
        <Button fx:id="join" mnemonicParsing="false" onAction="#join" prefHeight="16.0" prefWidth="129.0" styleClass="button-join-staff" stylesheets="@dataMahasiswa_view.css" text="Join Staff Koperasi" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="55.0" />
